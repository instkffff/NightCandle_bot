{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs|lib|util|__tests__)"
  },
  "plugins": [
    "util/jsdoc/esi-param",
    "util/jsdoc/esi-returns",
    "util/jsdoc/esi-route",
    "util/jsdoc/esi-example",
    "util/jsdoc/esi-description",
    "util/jsdoc/esi-homepage",
    "util/jsdoc/esi-all-routes",
    "plugins/markdown"
  ],
  "templates": {
    "systemName": "Eve Swagger JS",
    "theme": "lumen",
    "syntaxTheme": "default",
    "navType": "inline",

    "collapseSymbols": false,
    "includeDate": false,
    "inverseNav": true,
    "linenums": true,
    "search": true,
    "sort": true
  },
  "opts": {
    "template": "node_modules/ink-docstrap/template",
    "destination": "docs/",
    "encoding": "utf8",
    "private": false,
    "recurse": true
  }
}
